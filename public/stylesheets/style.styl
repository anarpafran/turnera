body
	padding: 0px
	margin: 0px
	font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
	color: #00B7FF

li
	list-style: url("/images/chulo.png")

.logo
	background: #F24C27
	height: 100px
	padding: 15px
	text-align: center	

.logo img
	padding: 15px
	width: 25%	

.cuerpo
	background: #037E8C
	color: white
	padding: 5px;	

.campo, .cta
	display: block
	height: 50px
	margin: 15px
	width: 400px
	font-size: 24px

.cta
	background: #F24C27
	border: none
	color: #FFFFFF

.beneficios
	background: white;

.mdo-titulo
	background: silver
	height: 100px
	padding: 15px	
	border-bottom: solid #747474 1px 

.mdo-cuerpo
	background: silver	
	margin: 0 20%

.mto-pie
	margin: 15px

.mto-pie > div
	float: left
	width: 32%
	margin: 3px


.mto-pie .estadistica
	font-weight: bold;

.mto-pie .estadistica span
	font-size: 35px	

.mto-pie .estadistica img
	height: 50px

.mto-pie .imprimir
	background: #26A5DD;
	color: white;
	text-align: center
	padding: 5px
	font-size: 23px;
	margin-top: 39px;

.mto-pie .opciones
	height: 100px	
	font-size: 23px

.mto-pie .opciones > div
	background: #000
	color: white
	float:left
	height: 100px
	margin: 3px
	width:100px
.opciones img
	margin: 27px;
.opciones span
	font-size: 14px
.turno_actual
	margin: 0px auto
	text-align:center

.turno_actual div
	color: white;
	background: #000000
	float: left
	font-size: 160px
	height: 185px
	margin: 1px
	width: 24.5%

.turno_accion div
	float:left
	width: 45.5%
	background: #000
	color: white
	font-size: 23px
	padding: 14px
	margin: 1px

.main
	background: silver
	
.reset
	clear:both;
